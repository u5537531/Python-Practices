def check(num):
    print("Welcome here! Please check your temperature!")
    if num <= 37.5:
       print(f"On the temperature check, your temperature is: {num} degree, health!")
    else:
        print(f"On the temperature check, your temperature is: {num} degree, You have a fever!")
check(38)


def add(a, b):
    result = a + b
    return result
r = add(5, 6)
print(r)


def check_age(age):
    if age > 18:
        return "adults"
    else: return None
result = check_age(16)
if not result:
    print("You are not adults!")